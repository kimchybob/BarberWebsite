<%@ page import="java.util.ArrayList" %><%@ page import="database.databaseAccess" %><%--  Created by IntelliJ IDEA.  User: kimchy  Date: 19/10/20  Time: 02:00  To change this template use File | Settings | File Templates.--%><%@ page contentType="text/html;charset=UTF-8" language="java" %><html><head>    <title>allAppointment</title></head><body><%    String userId = request.getParameter("userId");    ArrayList<ArrayList> result = new databaseAccess().searchAllAppointment();    ArrayList<String> nameOfClient = result.get(0);    ArrayList<String> serviceType = result.get(1);    ArrayList<String> time = result.get(2);    ArrayList<String> location = result.get(3);    ArrayList<String> message = result.get(4);%><table>    <tr>        <td>Name Of Client</td>        <td>Service Type</td>        <td>Time</td>        <td>Location</td>        <td>Message</td>    </tr>    <%        for(int i = 0; i < serviceType.size(); i++){    %>    <tr>        <td><%=nameOfClient.get(i)%></td>        <td><%=serviceType.get(i)%></td>        <td><%=time.get(i)%></td>        <td><%=location.get(i)%></td>        <td><%=message.get(i)%></td>    </tr>    <%        }    %></table><form action="admin.jsp">    <button type="submit" name="userId" value="<%=userId%>">back</button></form></body></html>