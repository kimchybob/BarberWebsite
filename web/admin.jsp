<%@ page contentType="text/html;charset=UTF-8" language="java" %><%@ page import="database.databaseAccess" %><html><head>    <%        String userId = (String)request.getAttribute("userId");        System.out.println(userId);    %>    <title>admin</title>    Welcome Admin <%=userId%></head><%    String[][] result = new databaseAccess().searchService();%><body><h2>Dashboard</h2><table>    <tr>        <th>Service Type</th>        <th>Charge</th>    </tr>    <%        for(int i = 0; i < result[0].length; i++){    %>    <tr>        <td>            <%=result[i][0]%>        </td>        <td>            <%=result[i][1]%>        </td>    </tr>    <%        }    %>    <tr>        <form action="admin" method="post">            <td><input type="text" placeholder="Service Type" name="serviceType"></td>            <td><input type="text" placeholder="Service Charge" name="serviceCharge"></td>            <td><button type="submit">Add new Service</button></td>        </form>    </tr></table><br><form action="allAppointment.jsp">    <button type="submit" name="userId" value="<%=userId%>">View All Appointments</button></form><br><form action="login.jsp">    <button type="submit">Logout</button></form></body></html>