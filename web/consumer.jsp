<%@ page import="java.util.ArrayList" %><%@ page import="database.databaseAccess" %><%--  Created by IntelliJ IDEA.  User: kimchy  Date: 14/10/20  Time: 16:02  To change this template use File | Settings | File Templates.--%><%@ page contentType="text/html;charset=UTF-8" language="java" %><html><%    String userId = (String)request.getAttribute("userId");%><head>    <title>consumer</title>    Welcome Consumer <%=userId%></head><body><div>    <h2>My appointments</h2>    <%        ArrayList<ArrayList> result = new databaseAccess().searchAppointment(userId);        ArrayList<String> serviceType = result.get(0);        ArrayList<String> time = result.get(1);        ArrayList<String> location = result.get(2);        ArrayList<String> message = result.get(3);        ArrayList<String> appointmentId = result.get(4);    %>    <table>        <tr>            <td>Service Type</td>            <td>Time</td>            <td>Location</td>            <td>Message</td>        </tr>        <%            for(int i = 0; i < serviceType.size(); i++){        %>        <tr>            <td><%=serviceType.get(i)%></td>            <td><%=time.get(i)%></td>            <td><%=location.get(i)%></td>            <td><%=message.get(i)%></td>            <td>                <form action="booking" method="get">                    <button type="submit" name="appointmentId" value="<%=appointmentId.get(i)%>,<%=userId%>">cancel</button>                </form>            </td>        </tr>        <%            }        %>    </table></div><form action="appointment.jsp">    <button type="submit" name="userId" value="<%=userId%>">make a new appointment</button><br></form><form action="information_update.jsp">    <button type="submit" name="userId" value="<%=userId%>">update personal information</button><br></form><form action="login.jsp">    <button type="submit">Logout</button></form></body></html>